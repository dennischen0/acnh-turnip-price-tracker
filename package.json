{
   "name": "acnhtpt_server",
   "version": "0.1.0",
   "private": true,
   "engines": {
      "node": "12.16.x"
   },
   "dependencies": {
      "@types/express": "^4.17.6",
      "@types/node": "^13.11.1",
      "auth0": "^2.24.0",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-bearer-token": "^2.4.0",
      "express-jwt": "^5.3.3",
      "jwks-rsa": "^1.8.0",
      "mysql": "^2.14.1",
      "npm-run-all": "^4.1.5",
      "reflect-metadata": "^0.1.10",
      "ts-node": "3.3.0",
      "typeorm": "0.2.25",
      "typescript": "^3.8.3"
   },
   "scripts": {
      "tsc": "tsc",
      "client": "cd client && npm start",
      "start": "ts-node server/server.ts",
      "start:watch": "nodemon",
      "build": "cd client && npm i && npm run build",
      "dev": "npm-run-all --parallel client start:watch"
   },
   "proxy": "http://localhost:8080",
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "server"
      ],
      "exec": "npm start",
      "ext": "ts"
   }
}
